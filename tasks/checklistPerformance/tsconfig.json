{
    "compilerOptions": {
      "target": "es5", 
      "lib": ["es5", "dom"],
      "module": "AMD",
      "moduleResolution": "node",
      "removeComments": true,
      "strict": true,
      "outDir": "dist/",
      "rootDir": "src/",
      "types": ["vss-web-extension-sdk"],
      "esModuleInterop": true,
      "sourceMap": true,
      "noImplicitAny": false
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }